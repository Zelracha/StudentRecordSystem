@model IEnumerable<StudentRecordSystem.Models.Student> // Update model type


    
<h1>Student List</h1>

<button type="button" class="btn btn-primary"
        onclick="location.href='@Url.Action("Add", "Student" )'">
    Add Student
</button>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Date of Birth</th>
            <th>Email</th>
            <th>Phone</th>

        </tr>
    </thead>
    <tbody>
        @foreach (StudentRecordSystem.Models.Student s in Model)
        {
            <tr>
                <td>@s.StudentId</td>
                <td>@(s.FirstName + " " + s.LastName)</td>
                <td>@s.DisplayDateOfBirth</td>
                <td>@s.Email</td>
                <td>@s.Phone</td>
                <td>
                    <button type="button" class="btn btn-primary"
                            onclick="location.href='@Url.Action("Add", "Student" , new {id = s.StudentId} )'">
                        Edit
                    </button>
                </td>

            </tr>
        }
    </tbody>
</table>